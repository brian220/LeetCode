// Easy
// Can be solved by double pointers
class Solution {
public:
    void swap(int &a, int &b) {
        int temp = a;
        a = b;
        b = temp;
    }
    
    int removeElement(vector<int>& nums, int val) {
        int cur = 0;
        for (int i = 0; i < nums.size(); i ++) {
            if (nums[i] != val) {
                swap(nums[cur], nums[i]);
                cur ++;
            }
        }
        return cur;
    }
};
